pipeline {
    agent any // This specifies that the pipeline can run on any available agent (slave node) in your Jenkins environment.
    
    stages {
        stage("GIT") { // This defines a stage named "GIT".
            steps {
                echo "Getting Project from Git" // This prints a message to the Jenkins console.
                git branch: "master", // This specifies the branch to checkout from the Git repository.
                    url: "https://github.com/Hanine122/Achat" // This specifies the URL of the Git repository.
            }
        }
    }
}
